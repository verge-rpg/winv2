void CenterString(int y, string text)
{
int x;
x=strlen(text)*7/2;
x=160-x;
GotoXY(x, y);
PrintString(0, text);
}

void CenterStringX(int x1, int y, string text)
{
int x;
x=strlen(text)*7/2;
x=x1-x;
GotoXY(x, y);
PrintString(0, text);
}

